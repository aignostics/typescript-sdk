{
  "name": "@aignostics/platform-typescript-sdk",
  "version": "0.0.0-development",
  "description": "The TypeScript SDK for the Aignostics Platform",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "aignostics-platform": "./dist/cli/index.cjs"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run codegen && npm run compile",
    "compile": "tsup",
    "codegen": "npm run codegen:clean && npm run codegen:generate",
    "codegen:clean": "rm -rf src/generated",
    "codegen:generate": "docker run --rm -v ${PWD}:/local --user $(id -u):$(id -g) openapitools/openapi-generator-cli:v7.14.0 generate -i https://raw.githubusercontent.com/aignostics/python-sdk/refs/heads/main/codegen/in/api.json -g typescript-axios -o /local/src/generated --additional-properties=supportsES6=true,withInterfaces=true,typescriptThreePlus=true,useSingleRequestParameter=true,importFileExtension=.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "docs": "typedoc src/index.ts --out docs --exclude '**/*.test.ts' --exclude 'src/generated/**'",
    "docs:serve": "npx http-server docs -p 8080",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rm -rf dist docs coverage",
    "prepublishOnly": "npm run build && npm test",
    "test:compatibility": "./scripts/test-node-compatibility.sh",
    "test:compatibility:docker": "./scripts/test-node-compatibility-docker.sh",
    "test:quick": "./scripts/quick-test.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aignostics/platform-typescript-sdk.git"
  },
  "keywords": [
    "aignostics",
    "platform",
    "sdk",
    "typescript",
    "api"
  ],
  "author": "Aignostics",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aignostics/platform-typescript-sdk/issues"
  },
  "homepage": "https://github.com/aignostics/platform-typescript-sdk#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/express": "^5.0.3",
    "@types/keytar": "^4.4.0",
    "@types/node": "^24.0.13",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.2.1",
    "msw": "^2.10.4",
    "prettier": "^3.4.2",
    "semantic-release": "^21.1.2",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.7",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@napi-rs/keyring": "^1.1.8",
    "axios": "^1.7.9",
    "express": "^5.1.0",
    "keytar": "^7.9.0",
    "open": "^10.2.0",
    "openid-client": "^5",
    "yargs": "^17.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
